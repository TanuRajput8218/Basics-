import java.util.*;
public class classroom {
        public static void main(String[] args) {
            Scanner sc = new Scanner(System.in);

           int num  = sc.nextInt();


           if (num< 0) {
               System.out.println("num is negaitive");
           }
           else if(num >0) {
               System.out.println("num is positive");
           }
           else {
                System.out.println("num is 0");

            }
        }
    }

