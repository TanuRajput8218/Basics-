import java.util.*;
public class classroom {
        public static void main(String[] args) {
            Scanner sc = new Scanner(System.in);

           int A  = sc.nextInt();
           int B = sc.nextInt();
           if (A>B) {
               System.out.println("A is largest");
           }
           else {


                System.out.println("B is largest");

            }
        }
    }

